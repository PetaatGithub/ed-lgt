[IO-handlers]
dat = results/qed/$uid/$key, w, simsio.serializers.NPZSerializer
log = results/qed/$uid/info,  , simsio.serializers.LogSerializer
par = results/qed/$uid/par, w, simsio.serializers.YAMLSerializer
res = results/qed/$uid/res, w, simsio.serializers.NPZSerializer

[configs]
directory = configs
header_tag = ===
header_ref = <<<
lock_attempts = 100

[versioning]
git_revision = git rev-parse HEAD

[logging-format]
format = %(asctime)s %(levelname)-8s | %(message)s
datefmt = %Y-%m-%d %H:%M

[logging-levels]
root = INFO
simsio.iocore = WARNING
