[IO-handlers]
dat = data/$uid/$key, w, simsio.serializers.PickleSerializer
log = Results/SU2/$uid,  , simsio.serializers.LogSerializer
par = Results/SU2/$uid, w, simsio.serializers.YAMLSerializer
res = Results/SU2/$uid, w, simsio.serializers.NPZSerializer

[configs]
directory = Configs
header_tag = ===
header_ref = <<<
lock_attempts = 100

[versioning]
git_revision = git rev-parse HEAD

[logging-format]
format = %(asctime)s %(levelname)-8s | %(message)s
datefmt = %Y-%m-%d %H:%M

[logging-levels]
root = INFO
simsio.iocore = WARNING
